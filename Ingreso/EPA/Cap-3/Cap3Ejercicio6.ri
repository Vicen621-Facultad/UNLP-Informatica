programa Cap3Ejercicio6
areas
  ciudad: AreaC(1,1,100,100)
robots
  robot robot1
  variables
    flores: numero
  comenzar
    Pos(1, 7)
    derecha
    flores := 0

    mientras flores < 20
      mientras HayFlorEnLaEsquina
        flores := flores + 1
      mover

    Informar(flores)
  fin
variables
  R-info: robot1
comenzar
  AsignarArea(R-info,ciudad)
  Iniciar(R-info,1,1)
fin