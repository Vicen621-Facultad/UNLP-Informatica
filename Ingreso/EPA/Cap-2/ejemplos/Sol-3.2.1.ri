programa Sol-2.3.2.1
areas
  ciudad: AreaC(1,1,100,100)
robots
  robot robot1
  comenzar
    mover
    si HayPapelEnLaBolsa {Fijarse si hay papel en la bolsa para poder depositarlo}
      depositarPapel
    mover
    si HayFlorEnLaBolsa
      depositarFlor
  fin
variables
  R-info: robot1
comenzar
  AsignarArea(R-info,ciudad)
  Iniciar(R-info,1,1)
fin